{
  "name": "originphp/schedule",
  "description": "OriginPHP Task Scheduler",
  "type": "library",
  "keywords": [
    "cron",
    "task",
    "schedule"
  ],
  "homepage": "https://www.originphp.com/",
  "license": "MIT",
  "authors": [
    {
      "name": "Jamiel Sharief",
      "email": "js@originphp.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Origin\\Schedule\\": "."
    }
  },
  "require": {
    "php": ">=7.3.0",
    "originphp/core": "^3.19",
    "originphp/configurable": "^2.0",
    "originphp/process": "^3.19"
  },
  "suggest": {
    "originphp/job": "If you are going to queue events"
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
