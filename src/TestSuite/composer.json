{
  "name": "originphp/test-suite",
  "description": "OriginPHP Test Suite",
  "type": "library",
  "keywords": [
    "test suite",
    "integration testing"
  ],
  "homepage": "https://www.originphp.com/",
  "license": "MIT",
  "authors": [
    {
      "name": "Jamiel Sharief",
      "email": "js@originphp.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Origin\\TestSuite\\": "."
    }
  },
  "require": {
    "php": ">=7.3.0",
    "originphp/core": "^3.0",
    "originphp/inflector": "^2.0",
    "originphp/model": "^3.0",
    "phpunit/phpunit": "^9.2"
  },
  "suggest": {
    "originphp/http": "If you are going to use integration testing for web based applications",
    "originphp/console": "If you are going to use integration testing for console based commands",
    "originphp/job": "If you want to test queued jobs"
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
