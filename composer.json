{
  "name": "originphp/framework",
  "description": "The OriginPHP framework",
  "type": "library",
  "keywords": [
    "originPHP",
    "framework",
    "mvc",
    "easy",
    "simple",
    "convention over configuration",
    "orm",
    "docker",
    "rapid"
  ],
  "homepage": "https://www.originphp.com",
  "license": "MIT",
  "authors": [
    {
      "name": "Jamiel Sharief",
      "email": "js@originphp.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Origin\\": "src/"
    },
    "files": [
      "src/Core/functions.php",
      "src/I18n/functions.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Origin\\Test\\": "tests/"
    }
  },
  "require": {
    "php": ">=7.3.0",
    "originphp/defer": "^3.0",
    "originphp/configurable": "^1.0|^2.0",
    "originphp/inflector": "^1.0|^2.0",
    "originphp/security": "^1.0|^2.0",
    "originphp/cache": "^1.0|^2.0",
    "originphp/email": "^1.0|^2.0",
    "originphp/html": "^1.0|^2.0",
    "originphp/xml": "^1.0|^2.0",
    "originphp/log": "^1.0|^2.0",
    "originphp/validation": "^1.0|^2.0",
    "originphp/dotenv": "^1.0|^2.0",
    "ext-json": "*",
    "ext-mbstring": "*",
    "ext-openssl": "*",
    "ext-intl": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.2",
    "phpstan/phpstan": "^0.12.64"
  },
  "replace": {
    "originphp/console": "self.version",
    "originphp/core": "self.version",
    "originphp/http": "self.version",
    "originphp/i18n": "self.version",
    "originphp/job": "self.version",
    "originphp/lock": "self.version",
    "originphp/mailbox": "self.version",
    "originphp/mailer": "self.version",
    "originphp/migration": "self.version",
    "originphp/model": "self.version",
    "originphp/process": "self.version",
    "originphp/publisher": "self.version",
    "originphp/schedule": "self.version",
    "originphp/service": "self.version",
    "originphp/test-suite": "self.version",
    "originphp/utility": "self.version",
    "originphp/ssh": "self.version"
  },
  "suggest": {
    "ext-redis": "Redis",
    "ext-imap": "Imap",
    "ext-mailparse": "Mailparse",
    "ext-ssh2": "SSH2 extension"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "sort-packages": true
  }
}
